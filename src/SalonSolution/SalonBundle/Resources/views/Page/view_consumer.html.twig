{% include '::header_salon.html.twig' %}

<link href="{{ asset('theme/backend/assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.css') }}"  rel="stylesheet" type="text/css" media="all"/>
<link href="{{ asset('theme/backend/assets/admin/pages/css/profile-old.css') }}"  rel="stylesheet" type="text/css" media="all"/>

<!-- BEGIN CONTAINER -->
<div class="page-container">
	{% include '::sidebar_salon.html.twig' %}

	<div class="page-content-wrapper">
		<div class="page-content">
			


			<!-- END PAGE HEADER-->
			<!-- BEGIN PAGE CONTENT-->
		
		
		<div class="row profile">
				<div class="col-md-12">
					<!--BEGIN TABS-->
					<div class="tabbable-line tabbable-full-width">
						<ul class="nav nav-tabs">
							<li class="active">
								<a href="#tab_1_1" data-toggle="tab" aria-expanded="true">
								Overview </a>
							</li>						
						</ul>
						<div class="tab-content">
							<div class="tab-pane active" id="tab_1_1">
								<div class="row">
							{% for view in consumerDetails  %}
									<div class="col-md-3">
										<ul class="list-unstyled profile-nav">
											<li>
												 {% if view.photo  %}
													 		<img  class="img-circlemy" src="{{ asset('uploads/consumers/') }}{{ view.photo }}"/>	
													 
													   {% else %}													   
													   	<img src="{{ asset('uploads/default_image/') }}default_avatar.png" class="img-responsive" alt="">
													    
													   {% endif %}
											
												
											</li>
											
										</ul>
									</div>
									
									
										<div class="col-md-9">
										<div class="row">
											<div class="col-md-8 profile-info"> 
												<h1>{{ view.firstName }}    {{ view.lastName }} </h1>  
												<p>
														{{ view.username }}
													 													    
													  
												</p>
												
												<ul class="list-inline">
													<li>
														<i class="fa fa-map-marker"></i> {{ view.username }}
													</li>
													<li>
														<i class="fa-automobile"></i> {{ view.address }}
													</li>
													<li>
														<i class="fa fa-briefcase"></i> {{ view.city }}
													</li>
													<li>
														<i class="fa fa-star"></i> {{ view.state }}
													</li>
													<li>
														<i class="fa fa-heart"></i>  {{ view.zip }} 
													</li>
												</ul>
											</div>
											<!--end col-md-8-->
										
										</div>
										<!--end row-->
										<div class="tabbable-line tabbable-custom-profile">
											<ul class="nav nav-tabs">
												<li class="active">
													<a href="#tab_1_11" data-toggle="tab" aria-expanded="true">
													Latest Customers </a>
												</li>
												<!-- <li class="">
													<a href="#tab_1_22" data-toggle="tab" aria-expanded="false">
													Feeds </a>
												</li> -->
											</ul>
											<div class="tab-content">
												<div class="tab-pane active" id="tab_1_11">
													<div class="portlet-body">
														<table class="table table-striped table-bordered table-advance table-hover">
														<thead>
														<tr>
															<th>
																<i class="fa fa-briefcase"></i> Name
															</th>
															<th>
																<i class="fa fa-paper-plane-o"></i> Country
															</th>
															<th>
																<i class="fa fa-automobile"></i> Province
															</th>
															<th>
																<i class="fa fa-space-shuttle"></i> City
															</th>
															<th>
																<i class="fa fa-space-shuttle"></i> Zip
															</th>
															<th>
																<i class="fa fa-tachometer"></i> Address
															</th>
															<th>
																<i class="fa fa-search"></i> Mobile
															</th>
															<th>
																<i class="fa fa-magic"></i> Landline
															</th>
														</tr>
														</thead>
														<tbody>
														<tr>
															<td>
																<a href="javascript:;">
															{{ view.firstName }}</a>
															</td>
															<td class="hidden-xs">
																{{ view.country }}
															</td>
															<td>
																{{ view.state }}
															</td>
															<td>
																{{ view.city }}
															</td>
															<td>
																{{ view.address }}
															</td>
															<td>
																{{ view.zip }}
															</td>
															
															<td>
																{{ view.mobile }}
															</td>
															
															<td>
																{{ view.landline }}
															</td>
															
														</tr>
																												
														</tbody>
														</table>
													</div>
												</div>
												<!--tab-pane-->
											
											</div>
										</div>
									</div>
								
									{% endfor %}
							
								</div>
							</div>
							<!--tab_1_2-->
							
								<div class="form-actions">
									<div class="row">
										<div class="col-md-offset-3 col-md-9">
											<a href="{{ path('salon_solution_salon_manageConsumers') }}" class="btn btn-circle blue">Cancel </a>
										</div>
									</div>
								</div>
							
							<!--end tab-pane-->
							</div>
					</div>
					<!--END TABS-->
				</div>
			</div>
		
		
		
		
		
		
		
		
		
		</div>
	</div>
	<!-- END CONTENT -->
			
</div>
<!-- END CONTAINER -->
<script>
jQuery(document).ready(function() {       
   // initiate layout and plugins
   Metronic.init(); // init metronic core components
Layout.init(); // init current layout
QuickSidebar.init(); // init quick sidebar
Demo.init(); // init demo features
});
</script>
<script src="{{ asset('theme/backend/assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js') }}"  type="text/javascript"/></script>


{% include '::footer_salon.html.twig' %}
