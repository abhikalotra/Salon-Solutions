{% include '::header_admin.html.twig' %}


<link href="{{ asset('theme/backend/assets/global/plugins/jqvmap/jqvmap/jqvmap.css') }}"  rel="stylesheet" type="text/css" media="all"/>
<link href="{{ asset('theme/backend/assets/global/plugins/select2/select2.css') }}"  rel="stylesheet" type="text/css" media="all"/>
<link href="{{ asset('theme/backend/assets/global/plugins/datatables/plugins/bootstrap/dataTables.bootstrap.css') }}"  rel="stylesheet" type="text/css" media="all"/>

<div class="page-container">
	
   {% include '::sidebar_admin.html.twig' %}
    
    
   <div class="page-content-wrapper">
      <div class="page-content">
         <!-- BEGIN SAMPLE PORTLET CONFIGURATION MODAL FORM-->
         <div class="modal fade" id="portlet-config" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
               <div class="modal-content">
                  <div class="modal-header">
                     <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                     <h4 class="modal-title">Modal title</h4>
                  </div>
                  <div class="modal-body">
                     Widget settings form goes here
                  </div>
                  <div class="modal-footer">
                     <button type="button" class="btn blue">Save changes</button>
                     <button type="button" class="btn default" data-dismiss="modal">Close</button>
                  </div>
               </div>
               <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
         </div>
         <!-- /.modal -->
         <!-- END SAMPLE PORTLET CONFIGURATION MODAL FORM-->
         <!-- BEGIN PAGE HEADER-->
         <div class="page-bar">
            <ul class="page-breadcrumb">
               <li>
                  <i class="fa fa-home"></i>
                  <a href="{{ path('salon_solution_admin_dashboard') }}">Home</a>
                  <i class="fa fa-angle-right"></i>
               </li>
               <li>
                  <a href="{{ path('salon_solution_admin_manageUser') }}">Coustomer Tables</a>
                  <i class="fa fa-angle-right"></i>
               </li>
            
            </ul>
         </div>
         <!-- END PAGE HEADER-->
         <!-- BEGIN PAGE CONTENT-->
         <div class="row">
            <div class="col-md-12">
               <!-- BEGIN EXAMPLE TABLE PORTLET-->
               <div class="portlet box blue">
                  <div class="portlet-title">
                     <div class="caption">
                        <i class="fa fa-edit"></i>Coustomer Tables
                     </div>
                     <div class="tools">
                        <a href="javascript:;" class="reload">
                        </a>                      
                     </div>
                  </div>
                   
                  <div class="portlet-body">
                     <div class="table-toolbar">
                        <div class="row">
                           <div class="col-md-6">
                              <div class="btn-group">
                               
                              </div>
                           </div>
                       
                        </div>
                     </div>
                     <table class="table table-striped table-hover table-bordered" id="sample_editable_1">
                        <thead>
                           <tr>
                              <th>Id</th>
                              <th>Name</th>        
                                <th>Username</th>
                              <th>Email</th>
                              <th>Status</th>
                              <th>Action</th>                             
                           </tr>
                        </thead>
                        <tbody>
							
						{% for fetch_data in fetchCoustomer  %}
							<tr>
								
                              <td>  {{ fetch_data.id }}  </td>
                              <td> 	{{ fetch_data.firstName }}  	{{ fetch_data.lastName }}  </td>
                              <td>   {{ fetch_data.username }}    </td>
                              <td class="center">  {{ fetch_data.email }}  </td>
                              
                              {% if fetch_data.status == 1   %}
								<td class="center" id="divStatus-{{fetch_data.id}}">
									<a id="status-{{fetch_data.id}}" class="activate" title="Click to Change" onclick="javascript:changeStatus('status-{{fetch_data.id}}',{{fetch_data.status}});">   
										Active
									</a> 
							   </td>
							  {% else %}
								<td class="center" id="divStatus-{{fetch_data.id}}">  
									<a id="status-{{fetch_data.id}}" class="edit" title="Click to Change" onclick="javascript:changeStatus('status-{{fetch_data.id}}',{{fetch_data.status}});">  
										Inactive 
									</a>  
								</td>
								{% endif %}
							  
                             
                              
                              <td> <a class="edit" href="{{ asset('admin/editManageCoustumer') }}/{{ fetch_data.id }}"> Edit </a> 
                              <a class="delete" href="{{ asset('admin/deleteManageCoustumer') }}/{{ fetch_data.id }}">  Delete </a> </td>
                           </tr>
							
								{% endfor %}
						
                        </tbody>
                     </table>
                  </div>
               </div>
               <!-- END EXAMPLE TABLE PORTLET-->
            </div>
         </div>
         <!-- END PAGE CONTENT -->
      </div>
   </div>
   <!-- END CONTENT -->
 
</div>
<!-- END CONTAINER -->
{% include '::footer_admin.html.twig' %}
<script>
   jQuery(document).ready(function() {       
      Metronic.init(); // init metronic core components
   Layout.init(); // init current layout
   QuickSidebar.init(); // init quick sidebar
   Demo.init(); // init demo features
      TableEditable.init();
   });
   
</script>


<script src="{{ asset('theme/backend/assets/assets/global/plugins/select2/select2.min.js') }}"  type="text/javascript"/></script>
<script src="{{ asset('theme/backend/assets/global/plugins/datatables/media/js/jquery.dataTables.min.js') }}"  type="text/javascript"/></script>
<script src="{{ asset('theme/backend/assets/global/plugins/datatables/plugins/bootstrap/dataTables.bootstrap.js') }}"  type="text/javascript"/></script>
<script src="{{ asset('theme/backend/assets/admin/pages/scripts/table-editable.js') }}"  type="text/javascript"/></script>
